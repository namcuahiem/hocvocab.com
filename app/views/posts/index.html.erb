<h1>List of all posts</h1>
	<div class="list-group">
	<% @posts.each do |post| %>
		<a href="<%= post_path(post)%>" class="list-group-item">
            <h4 class="list-group-item-heading"><%= post.title %></h4>
            <p class="list-group-item-text"><%= post.content %></p>
        </a>
	<% end %>
</div>

        
